<?xml version="1.0" ?>
<!--# # -->

<Problem>

  <Solvers
    gravityVector="0.0,0.0,-9.81">
    <ReactiveTransport
      name="ReactiveTransport"
      logLevel="1"
      discretization="singlePhaseTPFA"
      targetRegions="{ Region2 }"
      fluidNames="{ water }"
      reactiveFluidNames="{chem}"
      updatePorosity="0"
      solidNames="{ rock }">
      <NonlinearSolverParameters
        newtonTol="1.0e-5"
        newtonMaxIter="20"
        lineSearchAction="None"
        newtonMinIter="2"/>
      <LinearSolverParameters
        solverType="gmres"
        krylovTol="1.0e-12"/>
    </ReactiveTransport>

    
    <GeochemicalModel 
      name="GeochemicalModel"
      logLevel="1"
      discretization="singlePhaseTPFA"
      targetRegions="{Region2}"
      fluidNames="{water}"
      reactiveFluidNames="{chem}"
      solidNames="{rock}"
      fixedHplus="1"
      >

      <NonlinearSolverParameters
        newtonTol="1.0e-8"
        newtonMinIter="11"          
        newtonMaxIter="15"/>
      <LinearSolverParameters
        solverType="gmres"
        krylovTol="1.0e-12"/>          

    </GeochemicalModel>

    <SinglePhaseFVM
      name="SinglePhaseFVM"
      logLevel="1"
      discretization="singlePhaseTPFA"
      targetRegions="{ Region2 }"
      fluidNames="{ water }"
      solidNames="{ rock }">
      <NonlinearSolverParameters
        newtonTol="1.0e-4"
        newtonMaxIter="12"
        lineSearchAction="None"
        newtonMinIter="1"/>
      <LinearSolverParameters
        solverType="gmres"
        krylovTol="1.0e-6"/>
    </SinglePhaseFVM>


    <FlowReactiveTransport
      name="FlowReactiveTransport"
      reactiveTransportSolverName="ReactiveTransport"
      flowSolverName="SinglePhaseFVM"
      geochemicalModelName="GeochemicalModel"      
      targetRegions="{ Region2 }"
      logLevel="1">
      <NonlinearSolverParameters
        newtonMinIter="1"
        newtonMaxIter="1"/>
      <LinearSolverParameters/>
    </FlowReactiveTransport>

    
  </Solvers>
  
  <Mesh>
    <InternalMesh name="mesh1"
                  elementTypes="{C3D8}" 
                  xCoords="{0, 0.15}"
                  yCoords="{0, 0.01}"
                  zCoords="{0, 0.01}"
                  nx="{30}"
                  ny="{1}"
                  nz="{1}"
                  cellBlockNames="{cb1}"/>
  </Mesh>

  <Geometry>
    <Box
      name="source"
      xMin="-0.01, -0.01, -0.01"
      xMax=" 0.0051, 10.01, 1.01"/>
    <Box
      name="sink"
      xMin=" 0.1449, -0.01, -0.01"
      xMax=" 0.151, 10.01, 1.01"/>
  </Geometry>


  <Events maxTime="1e4">

    <!-- This event is applied every cycle, and overrides the
    solver time-step request -->
    <PeriodicEvent name="solverApplications"
                   forceDt="50"
                   target="/Solvers/FlowReactiveTransport" />

    <PeriodicEvent name="outputs"
                   timeFrequency="1e3"
                   targetExactTimestep="1e3"
                   target="/Outputs/siloOutput" />

  </Events>

  <NumericalMethods>
    <FiniteVolume>
      <TwoPointFluxApproximation name="singlePhaseTPFA"
                                 fieldName="pressure"
                                 coefficientName="permeability"/>
    </FiniteVolume>
  </NumericalMethods>

  <ElementRegions>
    <CellElementRegion name="Region2"
                   cellBlocks="{cb1}"
                   materialList="{water, chem, rock}"/>
                   
  </ElementRegions>

  <Constitutive>
    <CompressibleSinglePhaseFluid name="water"
                                  defaultDensity="1000"
                                  defaultViscosity="0.001"
                                  referencePressure="0.0"
                                  referenceDensity="1000"
                                  compressibility="5e-20"
                                  referenceViscosity="0.001"
                                  viscosibility="0.0"/>

    <PoreVolumeCompressibleSolid name="rock"
                                 referencePressure="0.0"
                                 compressibility="1e-9"/>

    <EquilibratedChemicalFluid name="chem"
                               databaseType="EQ36"
                               databaseFile="thermo-databases/data0.shv"
                               kineticReactionType="MineralReactions"
                               kineticReactionFile="mineralReactions.txt"
                               basisSpeciesNames="{Na+, Cl-, HCO3-, Ca++, H+}"
                               inputDependentSpeciesNames="{NaCl(aq), CaCO3(aq), CO2(aq), CaCl+, CaCl2(aq), CaHCO3+}"
                               useActCoef="0"
                               activityCoefModel="BDot"/>

    <!-- Note that default useActCoef is 1  -->    

  </Constitutive>


  <FieldSpecifications>
    <FieldSpecification
      name="permx"
      component="0"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region2/cb1"      
      fieldName="permeability"
      scale="1.0e-12"/>

    <FieldSpecification
      name="permy"
      component="1"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region2/cb1"
      fieldName="permeability"
      scale="1.0e-12"/>

    <FieldSpecification
      name="permz"
      component="2"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region2/cb1"
      fieldName="permeability"
      scale="1.0e-12"/>

    <FieldSpecification
      name="referencePorosity"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region2/cb1"
      fieldName="referencePorosity"
      scale="0.1"/>

    <FieldSpecification
      name="initialPorosity"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region2/cb1"
      fieldName="initialPorosity"
      scale="0.1"/>

    <FieldSpecification
      name="initialMineralSurfaceArea"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region2/cb1"
      fieldName="initialMineralSurfaceArea"
      component="0"                 
      scale="2.4573e+06"/>

    <FieldSpecification
      name="theta0"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region2/cb1"
      fieldName="initialMineralVolumeFraction"
      component="0"                       
      scale="0.1"/>

    <FieldSpecification
      name="theta"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region2/cb1"
      fieldName="mineralVolumeFraction"
      component="0"                       
      scale="0.1"/>

    <FieldSpecification
      name="thetabc1"
      initialCondition="1"
      setNames="{ source }"
      objectPath="ElementRegions/Region2/cb1"
      fieldName="mineralVolumeFraction"
      component="0"                       
      scale="0.0"/>

    <FieldSpecification
      name="thetabc2"
      initialCondition="1"
      setNames="{ sink }"
      objectPath="ElementRegions/Region2/cb1"
      fieldName="mineralVolumeFraction"
      component="0"                       
      scale="0.0"/>
    
    <FieldSpecification
      name="initialPressure"
      initialCondition="1"
      setNames="{ all }"
      objectPath="ElementRegions/Region2/cb1"
      fieldName="pressure"
      scale="1e5"/>

    <FieldSpecification
      name="sourceTerm"
      objectPath="ElementRegions/Region2/cb1"
      fieldName="pressure"
      scale="2e5"
      setNames="{ source }"/>

    <FieldSpecification
      name="sinkTerm"
      objectPath="ElementRegions/Region2/cb1"
      fieldName="pressure"
      scale="1e5"
      setNames="{ sink }"/>
    
    <FieldSpecification name="temperature"
               initialCondition="1"
               setNames="{all}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="temperature"
               scale="60.0"/>    

    <FieldSpecification name="totalNa+"
               initialCondition="1"
               setNames="{all}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="totalConcentration"
               component="0"           
               scale="1.065"/>

    <FieldSpecification name="totalCl-"
               initialCondition="1"
               setNames="{all}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="totalConcentration"
               component="1"           
               scale="1.086"/>

    <FieldSpecification name="totalHCO3-"
               initialCondition="1"
               setNames="{all}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="totalConcentration"
               component="2"           
               scale="7.92e-3"/>

    <FieldSpecification name="totalCa++"
               initialCondition="1"
               setNames="{all}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="totalConcentration"
               component="3"           
               scale="3.37E-02"/>     

    <FieldSpecification name="totalH+"
               initialCondition="1"
               setNames="{all}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="totalConcentration"
               component="4"           
               scale="7.4E-08"/>     

    <FieldSpecification name="totalNa+1"
               initialCondition="1"
               setNames="{source}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="totalConcentration"
               component="0"           
               scale="1.065"/>

    <FieldSpecification name="totalCl-1"
               initialCondition="1"
               setNames="{source}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="totalConcentration"
               component="1"           
               scale="1.086"/>

    <FieldSpecification name="totalHCO3-1"
               initialCondition="1"
               setNames="{source}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="totalConcentration"
               component="2"           
               scale="3.31000000E-01"/>

    <FieldSpecification name="totalCa++1"
               initialCondition="1"
               setNames="{source}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="totalConcentration"
               component="3"           
               scale="3.3700000010E-02"/>     

    <FieldSpecification name="totalH+1"
               initialCondition="1"
               setNames="{source}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="totalConcentration"
               component="4"           
               scale="5.9697E-05"/>     
    

    <FieldSpecification name="Na+1"
               setNames="{source}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="concentration"
               component="0"           
               scale="-5.179494568767917251e-02"/>

    <FieldSpecification name="Cl-1"
               setNames="{source}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="concentration"
               component="1"           
               scale="-4.839020636270108949e-02"/>

    <FieldSpecification name="HCO3-1"
               setNames="{source}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="concentration"
               component="2"           
               scale="-2.546392141657875197e+00"/>

    <FieldSpecification name="Ca++1"
               setNames="{source}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="concentration"
               component="3"           
               scale="-1.636711523192096118e+00"/>     
    
    <FieldSpecification name="H+1"
               setNames="{source}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="concentration"
               component="4"           
               scale="-4.224047493262299113e+00"/>     

    <FieldSpecification name="Na+2"
               setNames="{sink}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="concentration"
               component="0"           
               scale="-5.183025282396437616e-02"/>

    <FieldSpecification name="Cl-2"
               setNames="{sink}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="concentration"
               component="1"           
               scale="-4.817831709573922733e-02"/>

    <FieldSpecification name="HCO3-2"
               setNames="{sink}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="concentration"
               component="2"           
               scale="-2.297090826913271044e+00"/>

    <FieldSpecification name="Ca++2"
               setNames="{sink}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="concentration"
               component="3"           
               scale="-1.653112643782814750e+00"/>     

    <FieldSpecification name="H+3"
               setNames="{sink}"
               objectPath="ElementRegions/Region2/cb1"
               fieldName="concentration"
               component="4"           
               scale="-7.130768280269023762e+00"/>     
    

    
  </FieldSpecifications>

  <Functions>
  </Functions>

  <Outputs>
      <Silo name="siloOutput"/>
  </Outputs>


</Problem>
